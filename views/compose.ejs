<%- include("partials/header") -%>
<h1>Compose</h1>
<form action='/compose' method="post">
  <div class="form-group">
    <label for="tit">Title</label>
    <input type="text" name="postTitle" class='form-control' id='tit'>
  </div>
  <div class="form-group">
    <label for="titl">Subitle</label>
    <input type="text" name="postSubTitle" class='form-control' id='titl'>
  </div>
  <div class="form-group">
    <label for="pst">Post</label>
    <textarea class='form-control' id = 'pst' name="postBody" rows="8" cols="80"></textarea>
  </div>
  <div class="form-group">
    <label for="author">Author</label>
    <input type="text" class='form-control' id = 'author' name="postAuthor">
  </div>
  <button type="submit" class='btn btn-primary'>
     Publish
   </button>
</form>
<%- include("partials/footer") -%>
